<%- include("partials/header") %>

    <h1>Compose</h1>

    <form action="/compose" method="POST">
        <div class="form-group">
            <label for="postTitle">Title</label>
            <input type="text" class="form-control" id="postTitle" aria-describedby="postPara">
          </div>
          <div class="form-group">
            <label for="postPara">Post</label>
            <textarea class="form-control" name="postTitle" type="text" id="postPara" name="postParagraph" aria-label="With textarea"></textarea>
          </div>
        <button class="btn btn-primary" name="button" type="submit">Publish</button>
    </form>

